    <display-name>TestServlet</display-name>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>bjtu.gruop7.servlet.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/TestServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RequestReckoningServlet</display-name>
    <servlet-name>RequestReckoningServlet</servlet-name>
    <servlet-class>bjtu.gruop7.servlet.RequestReckoningServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestReckoningServlet</servlet-name>
    <url-pattern>/RequestReckoningServlet</url-pattern>
  </servlet-mapping>
</web-app>